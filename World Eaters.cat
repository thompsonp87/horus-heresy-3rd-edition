            <constraint type="min" value="1" field="selections" scope="parent" shared="true" id="d412-ead8-b9c6-5248"/>
            <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="0eaf-61ba-b2ff-a0ee"/>
